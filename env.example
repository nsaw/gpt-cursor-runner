# GPT-Cursor Runner Configuration
# Copy this file to .env and fill in your actual values

# GPT API Configuration
GPT_API_KEY=sk-proj-your-gpt-api-key-here

# Ngrok Configuration
NGROK_AUTHTOKEN=your-ngrok-auth-token-here
NGROK_PORT=5050

# Webhook Endpoint (will be updated with ngrok URL)
ENDPOINT_URL=https://your-ngrok-subdomain.ngrok-free.app/webhook

# Dashboard URL (for Slack dashboard command)
DASHBOARD_URL=https://your-ngrok-subdomain.ngrok-free.app/dashboard

# Cursor Project Path (optional)
CURSOR_PROJECT_PATH=/path/to/your/cursor/project

# Slack Configuration (optional)
SLACK_APP_ID=your-slack-app-id
SLACK_CLIENT_ID=your-slack-client-id
SLACK_CLIENT_SECRET=your-slack-client-secret
SLACK_SIGNING_SECRET=your-slack-signing-secret
SLACK_VERIFICATION_TOKEN=your-slack-verification-token
SLACK_BOT_TOKEN=xapp-your-slack-bot-token
SLACK_REDIRECT_URI=https://your-ngrok-subdomain.ngrok-free.app/slack/oauth/callback

# Slack Proxy Configuration (fallback when app install is blocked)
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL
SLACK_CHANNEL=#general
SLACK_USERNAME=GPT-Cursor Runner 